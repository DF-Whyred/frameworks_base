<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Max visible notification icons -->
    <integer name="config_maxVisibleNotificationIcons">4</integer>
    <integer name="config_maxVisibleNotificationIconsOnLock">3</integer>

    <!-- Color of the UDFPS pressed view -->
    <color name="config_udfpsColor">#ffffffff</color>

    <!-- Udfps vendor code -->
    <integer name="config_udfps_vendor_code">0</integer>

    <!-- Doze: does the double tap sensor need a proximity check? -->
    <bool name="doze_double_tap_proximity_check">false</bool>

    <!-- Doze: does the long press sensor need a proximity check? -->
    <bool name="doze_long_press_proximity_check">false</bool>

    <!-- Whether usage of the proximity sensor during doze is supported -->
    <bool name="doze_proximity_sensor_supported">true</bool>

    <!-- If not zero, limits the internal screen recorder's framerate to the set value. -->
    <integer name="config_screenRecorderMaxFramerate">0</integer>

    <!-- StatusBar burn-in protection -->
    <bool name="config_disableStatusBarBurnInProtection">false</bool>
    <integer name="config_shift_interval">60</integer>

    <!-- Whether show refresh rate QS tile -->
    <bool name="config_showRefreshRateQsTile">false</bool>

    <!-- The maximum number of tiles in a row -->
    <integer name="quick_qs_panel_max_tiles">5</integer>

    <!-- The number of columns in the QuickQSPanel -->
    <integer name="quick_qs_panel_num_columns">5</integer>
    <integer name="quick_qs_panel_num_columns_landscape">6</integer>
    <integer name="quick_qs_panel_num_columns_media">4</integer>

    <!--The number of columns in the QSPanel -->
    <integer name="qs_panel_num_columns">4</integer>
    <integer name="qs_panel_num_columns_landscape">6</integer>
    <integer name="qs_panel_num_columns_media">4</integer>

    <!-- This value is used when notification_scrim_transparent is set to true.
         Allows to change how transparent is the scrim notification area  -->
    <item name="config_notification_scrim_transparency_amount" translatable="false" format="float" type="dimen">
        0
    </item>

    <!-- This value is used when notification_scrim_transparent is set to true.
         Allows to change how transparent is the scrim background area (for example, in the header)  -->
    <item name="config_behind_scrim_transparency_amount" translatable="false" format="float" type="dimen">
        0
    </item>
</resources>
